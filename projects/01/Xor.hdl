/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a=a, b=b, out=orout);
    And(a=a, b=b, out=andout);
    Not(in=andout, out=notandout);
    And(a=orout, b=notandout, out=out);
}
